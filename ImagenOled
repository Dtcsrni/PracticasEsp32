void setup() {

 
  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 ha fallado en inicializarse"));
    for(;;);
  }
  delay(2000); //Pausa por 2 segundos
 
  // Limpiar pantalla.
  display.clearDisplay();
  
  // Dibujar el mapa de bits en pantalla
  display.drawBitmap(0, 0, image_data_PesoPluma, 128, 64, 1);
  display.display();
}
 
void loop() {
    // Scroll in various directions, pausing in-between:
  display.startscrollright(0x00, 0x0F);
  delay(2000);
  display.stopscroll();
  delay(1000);
  display.startscrollleft(0x00, 0x0F);
  delay(2000);
  display.stopscroll();
  delay(1000);
  display.startscrolldiagright(0x00, 0x07);
  delay(2000);
  display.startscrolldiagleft(0x00, 0x07);
  delay(2000);
  display.stopscroll();
  delay(1000);
}
